        <script src="/scripts/artist.js" type="module"></script>
      <% } %>
  <% } %>
  <title>Prism | <%= locals.title ? locals.title : "" %></title>
</head>
<body>
  <div class="main">
    <!-- <video loop autoplay muted class="background-vid">
      <source src="/videos/lights.mp4" type="video/mp4"/>
    </video> -->
    <%-include("../partials/header.ejs")%>
    <% if(locals.title){ %>
      <% if(locals.title === "Listen") { %>
        <%-include("../partials/song-display.ejs")%>
      <% } else { %>
        <%-include("../partials/artist-display.ejs")%>
      <% } %>
    <% } %>
  </div>
  
